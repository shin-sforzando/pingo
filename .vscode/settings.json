{
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": "explicit"
    }
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cSpell.words": [
    "adminsdk",
    "anthropics",
    "autodocs",
    "biomejs",
    "cloudbuild",
    "cloudsdktool",
    "clsx",
    "Commitizen",
    "Dasharray",
    "dont",
    "dotenv",
    "envlocal",
    "evenodd",
    "firebaseapp",
    "firebasestorage",
    "Firestore",
    "genai",
    "gitsecret",
    "gsutil",
    "hookform",
    "jsdom",
    "knip",
    "lefthook",
    "linecap",
    "linejoin",
    "lucide",
    "magicui",
    "manualstart",
    "Mcpjson",
    "Millis",
    "miterlimit",
    "murecho",
    "nextjs",
    "oklch",
    "participations",
    "pingo",
    "qrcode",
    "Repomix",
    "secretmanager",
    "shadcn",
    "subcollection",
    "subcollections",
    "turbopack",
    "vaul",
    "VIEWPORTS",
    "vite",
    "vitejs",
    "vitest",
    "WCAG",
    "xlink"
  ],
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "i18n-ally.displayLanguage": "ja",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["messages", "src/i18n"]
}
